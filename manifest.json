{
  "manifest_version": 3,
  "name": "My Vite Chrome Extension",
  "version": "1.0",
  "description": "A basic Chrome Extension using Vite setup",
 "icons": {
  "16": "icons/logo.png",
  "48": "icons/logo.png",
  "128": "icons/logo.png"
},
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.jsx"]
    }
  ]
}
